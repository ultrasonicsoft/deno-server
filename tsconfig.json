{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "http://*": [
        "$HOME/Library/Caches/deno/deps/http/*"
      ],
      "https://*": [
        "$HOME/Library/Caches/deno/deps/https/*"
      ],
    }
  },
  "include": [
    "./**/*.ts"
  ]
}